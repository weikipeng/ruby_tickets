<%= form_for(ticket) do |f| %>
    <% if ticket.errors.any? %>
        <div id="error_explanation">
          <h2><%= pluralize(ticket.errors.count, "error") %> prohibited this ticket from being saved:</h2>

          <ul>
            <% ticket.errors.full_messages.each do |message| %>
                <li><%= message %></li>
            <% end %>
          </ul>
        </div>
    <% end %>

    <div class="field">
      上传照片
      <%= f.file_field :image %>
    </div>

    <div class="field">
      上传文件
      <%= f.file_field :document_attachment %>
    </div>

    <div class="field">
      <%= f.label :name, '名字' %>
      <%= f.text_field :name %>
    </div>

    <div class="field">
      <%= f.label :set_id_seq, "座位号" %>
      <%= f.text_field :set_id_seq %>
    </div>

    <div class="field">
      <%= f.label :address, "地址" %>
      <%= f.text_area :address %>
    </div>

    <div class="field">
      <%= f.label :price_paid, "支付金额" %>
      <%= f.text_field :price_paid %>
    </div>

    <div class="field">
      <%= f.label :email_address, "邮箱" %>
      <%= f.text_field :email_address %>
    </div>

    <div class="field">
      <%= f.label :phone, "电话" %>
      <%= f.text_field :phone %>
    </div>

    <div class="actions">
      <%= f.submit %>
    </div>
<% end %>
